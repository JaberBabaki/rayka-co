<?xml version="1.0" encoding="utf-8"?>

<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">

        <shape>

            <gradient
                android:startColor="#ffffff"
                android:endColor="#ffffff" />

            <stroke
                android:width="2dp"
                android:color="#1A75BC" />


            <corners
                android:radius="10dp" />

            <padding
                android:left="2dp"
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp" />

        </shape>

    </item>
    
</selector>